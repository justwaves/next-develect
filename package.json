{
  "name": "next-develect",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier:write": "prettier --write \"**/*.{ts,js,tsx,jsx,html,css,json,yaml,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,js,tsx,jsx,html,css,json,yaml,md}\"",
    "lint:fix": "eslint \"**/*.{ts,js,tsx,jsx}\" --fix",
    "lint:check": "eslint \"**/*.{ts,js,tsx,jsx}\"",
    "prisma:generate": "dotenv -e .env.local pnpify prisma generate",
    "prisma:migrate": "dotenv -e .env.local pnpify prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local pnpify prisma studio",
    "prisma:seed": "sh prisma/seed.sh && dotenv -e .env.local pnpify prisma db seed",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "4.12.0",
    "@tanstack/react-query": "^4.29.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "next": "13.2.4",
    "next-auth": "^4.22.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@emotion/babel-plugin": "^11.10.6",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@next/eslint-plugin-next": "^13.2.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-styling": "^0.3.1",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@types/babel-plugin-macros": "^3",
    "@types/babel__core": "^7",
    "@types/node": "^18.15.0",
    "@types/prettier": "^2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@yarnpkg/pnpify": "^4.0.0-rc.42",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.1.0",
    "css-loader": "^6.7.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.4",
