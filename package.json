    "prisma": "4.12.0",
    "storybook-addon-next-router": "^4.0.2",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "twin.macro": "^3.1.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{ts,js,jsx,tsx,html,css,json,yaml,md}": "prettier --check",
    "**/*.{ts,js,tsx,jsx}": "eslint"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
